createPKI = caserver:.bash_history : CONTAINS : ./easyrsa init-pki
createCA = caserver:*.stdout : CONTAINS : CA creation complete. Your new CA certificate is at:
addCA = client:.bash_history : CONTAINS : sudo update-ca-certificates
createPriKey = client:.bash_history : CONTAINS : openssl genrsa -out catest-server.key
createCSR = client:.bash_history : CONTAINS : openssl req -new -key catest-server.key -out catest-server.req
_task1signingCSR = caserver:*.stdout : CONTAINS : Request successfully imported with short-name: catest-server
_task2signingCSR = caserver:*.stdout : CONTAINS : * /home/ubuntu/easy-rsa/pki/issued/catest-server.crt
